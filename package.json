{
  "name": "vemto",
  "version": "2.0.0",
  "description": "A Laravel code generator and studio",
  "main": "build/main/main.js",
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "build": "node scripts/build.js && electron-builder",
    "build:win": "node scripts/build.js && electron-builder --win",
    "build:mac": "node scripts/build.js && electron-builder --mac",
    "build:linux": "node scripts/build.js && electron-builder --linux",
    "lint": "eslint ./src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "lint",
    "devtools": "./node_modules/.bin/vue-devtools"
  },
  "repository": "https://github.com/deluze/electron-vue-template",
  "author": {
    "name": "Deluze",
    "url": "https://github.com/Deluze"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/cli": "^5.0.1",
    "@vue/devtools": "^6.2.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "electron": "^20.1.0",
    "electron-builder": "^23.3.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.23.1",
    "eslint-plugin-vue": "^9.5.1",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "vite-plugin-checker": "^0.5.1",
    "vue-tsc": "^0.40.13"
  },
  "dependencies": {
    "prettier": "^2.7.1",
    "vue": "^3.2.36",
    "vue-router": "4"
  }
}
