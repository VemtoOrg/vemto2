{
    "compilerOptions": {
        "strict": true,
        "noImplicitAny": false,
        "strictNullChecks": false,
        "strictPropertyInitialization": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "allowJs": true,
        "downlevelIteration": true,
        "paths": {
            "@Main/*": ["../main/*"],
            "@Common/*": ["./*"],
            "@Renderer/*": ["../renderer/*"],
            "@Tests/*": ["../../tests/*"],
        },
    }
}
